{
  "name": "mindsdb-rag-assistant",
  "version": "1.0.0",
  "description": "Intelligent e-commerce assistant with MindsDB RAG capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "start": "node -r dotenv/config dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest --run src/tests/e2e-integration.test.ts",
    "test:performance": "vitest --run src/tests/performance-load.test.ts",
    "test:e2e-full": "vitest --run src/tests/e2e-integration.test.ts src/tests/performance-load.test.ts",
    "test:security": "vitest --run src/security/__tests__/",
    "test:security:tenant": "vitest --run src/security/__tests__/TenantIsolationSecurity.test.ts",
    "test:security:pii": "vitest --run src/security/__tests__/PIIRedactionSecurity.test.ts",
    "test:security:encryption": "vitest --run src/security/__tests__/EncryptionSecurity.test.ts",
    "test:security:vpc": "vitest --run src/security/__tests__/VPCPenetrationTests.test.ts",
    "test:security:suite": "vitest --run src/security/__tests__/SecurityTestSuite.test.ts",
    "security:audit": "npm audit --audit-level=moderate",
    "security:report": "npm run test:security && echo 'Security report generated in security-report.json'",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit up:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "mindsdb:setup": "./scripts/setup-mindsdb-local.sh",
    "mindsdb:start": "./scripts/setup-mindsdb-local.sh start",
    "mindsdb:stop": "./scripts/setup-mindsdb-local.sh stop",
    "mindsdb:logs": "./scripts/setup-mindsdb-local.sh logs",
    "mindsdb:test": "./scripts/setup-mindsdb-local.sh test",
    "mindsdb:init-rag": "echo 'Run the SQL script: ./mindsdb_config/init_rag.sql in MindsDB Editor at http://localhost:47334'",
    "db:reset": "docker compose exec postgres psql -U postgres -c 'DROP DATABASE IF EXISTS mindsdb_rag;' && docker compose exec postgres psql -U postgres -c 'CREATE DATABASE mindsdb_rag;' && ./scripts/setup-db.sh",
    "db:seed": "node -r ts-node/register scripts/seed-database.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "setup": "./scripts/dev-setup.sh",
    "dev:docker": "./scripts/start.sh dev && npm run dev",
    "dev:tools": "./scripts/start.sh tools && npm run dev",
    "prod:docker": "./scripts/start.sh prod",
    "infra:deploy": "cd infrastructure/cdk && npm run deploy",
    "infra:destroy": "cd infrastructure/cdk && npm run destroy",
    "health": "curl -f http://localhost:3000/health || echo 'Service not running'",
    "clean": "rm -rf dist node_modules/.cache",
    "precommit": "npm run lint && npm run type-check && npm run test && npm run security:audit"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.400.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.400.0",
    "@aws-sdk/client-bedrock-runtime": "^3.400.0",
    "@aws-sdk/client-budgets": "3.907.0",
    "@aws-sdk/client-cloudtrail": "3.907.0",
    "@aws-sdk/client-cloudwatch": "3.907.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.400.0",
    "@aws-sdk/client-cognito-identity-provider": "3.907.0",
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-ecs": "3.907.0",
    "@aws-sdk/client-elasticache": "3.907.0",
    "@aws-sdk/client-kms": "^3.400.0",
    "@aws-sdk/client-qbusiness": "^3.400.0",
    "@aws-sdk/client-rds": "^3.400.0",
    "@aws-sdk/client-s3": "3.907.0",
    "@aws-sdk/client-secrets-manager": "^3.400.0",
    "@aws-sdk/client-sfn": "3.907.0",
    "@aws-sdk/client-sns": "3.907.0",
    "@aws-sdk/util-dynamodb": "^3.400.0",
    "@types/compression": "1.8.1",
    "aws-cdk-lib": "^2.100.0",
    "aws-jwt-verify": "5.1.1",
    "aws-sdk": "^2.1400.0",
    "axios": "1.12.2",
    "compression": "1.8.1",
    "constructs": "^10.3.0",
    "cors": "^2.8.5",
    "dotenv": "17.2.3",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "8.1.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.2.0",
    "multer": "^1.4.5-lts.1",
    "pg": "8.16.3",
    "postgres": "^3.4.0",
    "redis": "^4.6.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.155",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "5.1.3",
    "@types/jsonwebtoken": "9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.5.0",
    "@types/pg": "8.15.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "drizzle-kit": "^0.20.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.0"
  },
  "keywords": [
    "mindsdb",
    "rag",
    "aws",
    "ecommerce",
    "ai"
  ],
  "author": "",
  "license": "MIT"
}
